  "name": "dependabot-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/datetime": "^3.15.2",
    "@blueprintjs/select": "^3.12.0",
    "@blueprintjs/timezone": "^3.6.1",
    "@sentry/browser": "^5.15.1",
    "@svgr/webpack": "^5.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "bfj": "^7.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^3.0.0",
    "confusing-browser-globals": "^1.0.9",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "identity-obj-proxy": "^3.0.0",
    "immer": "^6.0.2",
    "lodash": "^4.17.14",
    "markdown-it": "^10.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "obj-str": "^1.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "qs": "^6.9.1",
    "react": "^16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-text-mask": "^5.4.3",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resolve": "^1.15.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.0.0",
    "uuid": "^7.0.2",
    "webpack": "^4.42.0",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "^3.0.0",
    "workbox-webpack-plugin": "^5.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "cypress": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "fetch-mock": "^9.3.1",
    "flow-bin": "^0.119.1",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.1",
    "prettier-stylelint": "^0.4.2",
    "react-dev-utils": "^10.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.0.0",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "start": "REACT_APP_GIT_SHA=`git rev-parse HEAD` node scripts/start.js",
    "build": "REACT_APP_GIT_SHA=\"${SOURCE_VERSION:-$(git rev-parse HEAD)}\" node scripts/build.js && cp build/index.html build/404.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --reporter junit --reporter-options \"mochaFile=cypress/results/e2e-test-output.xml\""
  },
  "engines": {
    "node": "10.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "safari > 6",
    "ie > 9",
    "ios > 6",
    "android > 4.3",
    "samsung > 3",
    "chromeandroid > 50"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
